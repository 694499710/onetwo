<?xml version="1.0" encoding="UTF-8"?>
<template name="sheet" label="报名客户资料" datasource="#data" sizePerSheet="10">

		<rows>
		
			<row span="2" fieldStyle="alignment:ALIGN_CENTER;verticalAlignment:VERTICAL_CENTER;" fieldFont="boldweight:BOLDWEIGHT_BOLD">
				<fields>
					<field value="'报名客户资料'" rowspan="2" colspan="4"/>
				</fields>
			</row>
			<row type="title" fieldHeaderStyle="alignment:ALIGN_CENTER;verticalAlignment:VERTICAL_CENTER;" fieldHeaderFont="boldweight:BOLDWEIGHT_BOLD">
				<fields>
					<field name="主键" />
					<field name="卡号" />
					<field name="密码" />
					<field name="carbean" />
				</fields>
			</row>
			<row name="element" type="iterator" datasource="#sheet.datasource" span="cardBeans.size()"  fieldFont="boldweight:BOLDWEIGHT_BOLD"> 
				<fields>
					<field name="id" rowspan="cardBeans.size()"/>
					<field name="cardNo" rowspan="cardBeans.size()" value="cardNo+'--for--test--'+cardPwd"/>
					<field name="cardPwd" rowspan="cardBeans.size()"/>
					<field name="name" rootValue="cardBeans">
					</field>
				</fields>
			</row>
		</rows>

</template>
