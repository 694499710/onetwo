<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <configuration >
    <stop-on-errors>true</stop-on-errors>
    <date-format>yyyy-MM-dd HH:mm:ss</date-format>
    <wildcard>true</wildcard>
  </configuration>
  
  <mapping map-empty-string="false" >
  	<class-a>org.onetwo.common.utils.bean.Bean1</class-a>
  	<class-b>org.onetwo.common.utils.bean.Bean2</class-b>
  	
  	<field>
  		<a >list</a>
  		<b type="iterate" set-method="addTestBean2">list</b>
  		<b-hint>org.onetwo.common.utils.bean.TestBean2</b-hint>
  	</field>
  </mapping>
  
  <!-- 
  <mapping map-empty-string="true" map-null="false">
  	<class-a>org.onetwo.common.utils.bean.TestBean1</class-a>
  	<class-b>org.onetwo.common.utils.bean.TestBean2</class-b>
  </mapping> -->
  
</mappings>