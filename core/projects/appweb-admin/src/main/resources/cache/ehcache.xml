<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" name="appCache">

	<diskStore path="java.io.tmpdir/ehcache/iccardweb/app" />

	<!-- DefaultCache setting. -->
	<defaultCache maxEntriesLocalHeap="10000" eternal="false" timeToIdleSeconds="300" timeToLiveSeconds="600"
		overflowToDisk="true" maxEntriesLocalDisk="100000" />
	<cache name="METHOD_CACHE_ETERNAL" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" />
	<cache name="METHOD_CACHE" maxEntriesLocalHeap="10000" eternal="false"
		timeToIdleSeconds="300" timeToLiveSeconds="600" overflowToDisk="false" />
</ehcache>